<h1>Admin : Fill Data</h1>
 <%= form_for( @questions ) do |f| %>


  <div class="field">
    <%= f.label :question_text %><br />
    <%= f.text_field :question_text  %>
  </div>
  <div class="field">
    <%= f.label :option1 %><br />
    <%= f.text_field :option1 %>
  </div>
<div class="field">
    <%= f.label :option2 %><br />
    <%= f.text_field :option2 %>
  </div>
<div class="field">
    <%= f.label :option3 %><br />
    <%= f.text_field :option3 %>
  </div>
<div class="field">
    <%= f.label :option4 %><br />
    <%= f.text_field :option4 %>
  </div>
<div class="field">
    <%= f.label :option5 %><br />
    <%= f.text_field :option5 %>
  </div>
  <div class="field">
    <%= f.label :answer %><br />
    <%= f.text_area :answer %>
  </div>
  <div class="field">
    <%= f.label :expl %><br />
    <%= f.text_area :expl %>
  </div>
<div class="field">
    <%= f.label :admin_rating %><br />
    <%= f.text_field :admin_rating %>
<div class="actions">
    <%= f.submit "Submit" %>
  </div>
<% end %>



